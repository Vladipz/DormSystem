{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "eventService": {
        "ClusterId": "eventCluster",
        "Match": {
          "Path": "/events/{**catch-all}"
        }
      },
      "authService": {
        "ClusterId": "authCluster",
        "Match": {
          "Path": "/api/Auth/{**catch-all}"
        }
      },
      "userService": {
        "ClusterId": "authCluster",
        "Match": {
          "Path": "/api/User/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "eventCluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5002/"
          }
        }
      },
      "authCluster": {
        "Destinations": {
          "authDestination": {
            "Address": "http://localhost:5001/"
          }
        }
      }
    }
  }
}
